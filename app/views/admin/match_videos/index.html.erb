<h1>Listing admin_match_videos</h1>

<%= render 'shared/video_search_form', search_path: admin_match_videos_path  %>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th><%= t 'activerecord.attributes.admin/matchvideo.year'  %></th>
      <th><%= t 'activerecord.attributes.admin/matchvideo.match'  %></th>
      <th><%= t 'activerecord.attributes.admin/matchvideo.round'  %></th>
      <th><%= t 'activerecord.attributes.admin/matchvideo.opponent'  %></th>
      <th><%= t 'activerecord.attributes.admin/matchvideo.part'  %></th>
      <th><%= t 'activerecord.attributes.admin/matchvideo.url'  %></th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_match_videos.each do |admin_match_video| %>
      <tr>
		<td><%= admin_match_video.year %></td>
		<td><%= admin_match_video.match.try(:name) %></td>
		<td><%= admin_match_video.round %></td>
		<td><%= admin_match_video.opponent %></td>
		<td><%= admin_match_video.part %></td>
		<td><%= link_to t('views.videos.watch'), watch_video_path(admin_match_video), target: '_blank' %></td>
        <td><%= link_to 'Show', admin_match_video %></td>
        <td><%= link_to 'Edit', edit_admin_match_video_path(admin_match_video) %></td>
        <td><%= link_to 'Destroy', admin_match_video, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Match video', new_admin_match_video_path %>
