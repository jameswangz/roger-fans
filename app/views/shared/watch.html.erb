<% if true %>
	<%= @video.year %> <%= @video.match.name %> Round <%= @video.round %> VS <%= @video.opponent  %>
	<iframe width=800 height=600 src="<%= @video.url %>" frameborder=0 allowfullscreen></iframe>
<% else %>	
	<embed src="<%= @video.url %>" allowFullScreen="true" quality="high" width="800" height="600" align="middle" allowScriptAccess="always" type="application/x-shockwave-flash"></embed>
<% end %>
